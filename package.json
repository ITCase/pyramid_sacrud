    }
  ],
  "engines": {
    "node": ">= 0.8.0"
  },
  "devDependencies": {
    "browser-sync": "^2.3.1",
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.3",
    "chai": "^2.1.2",
    "cheerio": "^0.19.0",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-batch": "~1.0.5",
    "gulp-concat": "^2.5.2",
    "gulp-minify-css": "^1.0.0",
    "gulp-newer": "^0.5.0",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.1.1",
    "main-bower-files": "^2.6.2",
    "mocha": "^2.2.1",
    "phantomjs": "^1.9.16",
    "selenium-webdriver": "2.44",
    "vinyl-buffer": "^1.0.0",
    "vinyl-map": "^1.0.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "scripts": {
    "test": "mocha -b --timeout 999999 pyramid_sacrud/static/js/tests/"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.min.js",
    "jquery-ui": "./bower_components/jquery-ui/ui/minified/jquery-ui.min.js",
    "jquery-maskedinput": "./bower_components/jquery-maskedinput/dist/jquery.maskedinput.js",
    "jqueryui-timepicker-addon": "./bower_components/jqueryui-timepicker-addon/dist/jquery-ui-timepicker-addon.js",
    "picker": "./bower_components/pickadate/lib/compressed/picker.js",
    "pickadate": "./bower_components/pickadate/lib/compressed/picker.date.js",
    "pickatime": "./bower_components/pickadate/lib/compressed/picker.time.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "jquery-ui": {
      "depends": "jquery"
    },
    "jquery-maskedinput": {
      "depends": "jquery"
    },
    "jqueryui-timepicker-addon": {
      "depends": "jquery-ui"
    },
    "picker": {
      "exports": "picker",
      "depends": "jquery"
    },
    "pickadate": {
      "exports": "pickadate",
      "depends": [
        "picker",
        "jquery"
      ]
    },
    "pickatime": {
      "exports": "pickatime",
      "depends": [
        "picker",
        "jquery"
      ]
    }
  }
}
