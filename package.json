  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "engines": {
    "node": ">= 0.8.0"
  },
  "devDependencies": {
    "browserify": "^7.0.0",
    "browserify-shim": "",
    "chai": "^1.10.0",
    "glob": "^4.3.1",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-concat": "^2.4.2",
    "gulp-minify-css": "^0.3.11",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^3.0.0",
    "jsdom": "^1.3.2",
    "minimatch": "^2.0.1",
    "selenium-webdriver": "^2.44.0",
    "underscore": "^1.7.0",
    "vinyl-source-stream": "^1.0.0",
    "main-bower-files": "~2.5.0",
    "gulp-filter": "~2.0.0",
    "vinyl-map": "~1.0.1",
    "gulp-clean": "~0.3.1"
  },
  "scripts": {
    "test": "mocha pyramid_sacrud/static/js/test/ --growl --bail --timeout 99999"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "jquery-ui": "./bower_components/jquery-ui/ui/jquery-ui.js",
    "jquery-maskedinput": "./bower_components/jquery-maskedinput/dist/jquery.maskedinput.js",
    "jqueryui-timepicker-addon": "./bower_components/jqueryui-timepicker-addon/dist/jquery-ui-timepicker-addon.js",
    "speakingurl": "./bower_components/speakingurl/speakingurl.min.js",
    "modernizr": "./bower_components/modernizr/modernizr.js",
    "Picker": "./bower_components/pickadate/lib/compressed/picker.js",
    "pickadate": "./bower_components/pickadate/lib/compressed/picker.date.js",
    "pickatime": "./bower_components/pickadate/lib/compressed/picker.time.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "jquery-ui": {
      "exports": null,
      "depends": "jquery"
    },
    "jquery-maskedinput": {
      "exports": null,
      "depends": "jquery"
    },
    "jqueryui-timepicker-addon": {
      "exports": null,
      "depends": "jquery-ui"
    },
    "speakingurl": {
      "exports": null,
      "depends": "jquery"
    },
    "modernizr": {
      "exports": "modernizr",
      "depends": "jquery"
    },
    "Picker": {
      "exports": "Picker",
      "depends": "jquery"
    },
    "pickadate": {
      "exports": "pickadate",
      "depends": [
        "Picker",
        "jquery"
      ]
    },
    "pickatime": {
      "exports": "pickatime",
      "depends": [
        "Picker",
        "jquery"
      ]
    }
  }
}
